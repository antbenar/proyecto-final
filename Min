#include <iostream>
#include <string.h>
#define Min2(a,b)((a>b)?b:a)
using namespace std;

template<typename T>

T Min(T a , T b){
    if (a > b)
    return b;
    else
    return a;
}


int main()
{
    int h= Min(7, 9);
    float j= Min2(5.7, 6.9);

    cout << h << endl;
    cout << j << endl;
    return 0;
}
